.result-list{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 10px;
  font-size: 16px;
}
.result-list-item-father{
  background-color: #ffffff;
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 10px;
}
.result-list-item{
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid rgb(246,246,246);
  margin:10px 5px 5px 15px
}
.result-list-item-image{
  width: 10px;
  height: 10px;
}
.result-list-item-father-title{
  color: rgb(0, 124, 210);
  font-weight: 600;
}
.result-list-bottom{
  background-color: rgb(255, 113, 9);
  color: #ffffff;
  margin: 40px;
  border-radius: 20px;
  padding: 10px 20px 10px 20px;
}
